---

import HeroWork from './HeroWork.astro';
import Testimonials from './Testimonials.astro';

const sectionTypes = {
  heroWork: HeroWork,
  testimonials: Testimonials,
};

const { section } = Astro.props;
const Component = sectionTypes[section.sectionType as keyof typeof sectionTypes];
---

{Component && <Component {...section.sectionData} />}